<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F3F3"
    tools:context=".Profile">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="95dp"
        android:layout_height="42dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="29dp"
        android:layout_marginBottom="820dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Profile"
        android:textColor="#212121"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ic_pesan"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="342dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="827dp"
        android:src="@drawable/ic_pesan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/lineView"
        android:layout_width="385dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="21dp"
        android:alpha="0.3"
        android:background="#212121"
        app:layout_constraintBottom_toTopOf="@+id/img_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/img_user"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="277dp"
        android:layout_marginBottom="694dp"
        android:contentDescription="User"
        android:src="@drawable/pic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/userNama"
        android:layout_width="160dp"
        android:layout_height="28dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="748dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Ester Theodora"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/img_user" />

    <TextView
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/poppins_medium"
        android:text="estertheodora@gmail.com"
        android:textColor="#817A7A"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/img_user" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="149dp"
        android:layout_marginTop="170dp"
        android:fontFamily="@font/poppins_regular"
        android:text="5 Follower"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="2dp"
        android:layout_height="12dp"
        android:layout_marginBottom="705dp"
        android:alpha="0.2"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="228dp"
        android:layout_marginTop="171dp"
        android:fontFamily="@font/poppins_regular"
        android:text="12 Following"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="General Setting"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="225dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="340dp"
        android:layout_height="205dp"
        android:background="@color/white"
        android:elevation="4dp"
        app:cardCornerRadius="8dp"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="252dp">

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:src="@drawable/ic_account"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="16dp"/>

        <TextView
            android:id="@+id/item_my_account"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="My Account"
            android:fontFamily="@font/poppins_medium"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="50dp" />

        <View
            android:id="@+id/lineView6"
            android:layout_width="340dp"
            android:layout_height="1dp"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="21dp"
            android:alpha="0.2"
            android:background="#212121"/>

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:src="@drawable/ic_address"
            android:layout_marginTop="62dp"
            android:layout_marginLeft="16dp"/>

        <TextView
            android:id="@+id/item_my_address"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="My Address"
            android:fontFamily="@font/poppins_medium"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="50dp"
            />

        <View
            android:id="@+id/lineView7"
            android:layout_width="340dp"
            android:layout_height="1dp"
            android:layout_marginTop="100dp"
            android:layout_marginBottom="21dp"
            android:alpha="0.1"
            android:background="#212121"/>

        <View
            android:id="@+id/lineView8"
            android:layout_width="340dp"
            android:layout_height="1dp"
            android:layout_marginTop="150dp"
            android:layout_marginBottom="21dp"
            android:alpha="0.1"
            android:background="#212121" />


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>
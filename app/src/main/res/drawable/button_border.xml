<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- Background Color -->
            <solid android:color="@color/white" />
            <!-- Border/Stroke -->
            <stroke
                android:width="2dp"
                android:color="#83bdde" />
            <!-- Corner Radius -->
            <corners android:radius="30dp" />
        </shape>
    </item>
</selector>
